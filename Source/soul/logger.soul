processor Logger
{
    // input event float<2> logIn;
    input event float<2> in;
    output event float<2> out;
    // output event float<2> test;

    float<2> log =float<2>(1, 0);

    event in(float<2> f)
    {
        log = f;
        // log.at(1) = 2;
    }

    void run()
    {
        loop
        {
            out << log;
            loop (50000)
            {
                advance();
            }
            // test << log.at(1);
        }
    }
}